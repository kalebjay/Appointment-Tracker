<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view_controller.MainMenuController">
   <children>
      <Label layoutX="129.0" layoutY="32.0" prefHeight="40.0" prefWidth="145.0" text="Main Menu">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Button fx:id="exitProgramBTN" layoutX="135.0" layoutY="330.0" mnemonicParsing="false" onAction="#exitProgram" text="Exit Program">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="reportsBTN" layoutX="155.0" layoutY="250.0" mnemonicParsing="false" onAction="#goToReportsView" text="Reports">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="appointmentsBTN" layoutX="125.0" layoutY="180.0" mnemonicParsing="false" onAction="#goToCalendarView" prefHeight="39.0" prefWidth="153.0" text="Appointments">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="customersBTN" layoutX="145.0" layoutY="110.0" mnemonicParsing="false" onAction="#goToCustomersView" text="Customers">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Separator layoutX="100.0" layoutY="300.0" prefWidth="200.0" />
   </children>
</AnchorPane>
